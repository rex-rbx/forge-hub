v27.Combat:AddToggle("SlapAura", {
    Title = "Auto Click Pillows",
    Description = "Auto clicks random pillows",
    Default = false,
    Callback = function(p28)
        autoslap = p28
        while autoslap do
            local v29, v30, v31 = pairs(workspace.map:GetChildren())
            while true do
                local v32
                v31, v32 = v29(v30, v31)
                if v31 == nil then
                    break
                end
                if v32:FindFirstChild("ClickDetector") then
                    fireclickdetector(v32.ClickDetector)
                end
            end
            local v33, v34, v35 = pairs(workspace.map.Model.Model:GetChildren())
            while true do
                local v36
                v35, v36 = v33(v34, v35)
                if v35 == nil then
                    break
                end
                if v36:FindFirstChild("ClickDetector") then
                    fireclickdetector(v36.ClickDetector)
                end
            end
            wait()
        end
    end
})
local vu37 = "hi"
v27.Combat:AddToggle("SlapAura", {
    Title = "Auto Spam Unit",
    Description = "Spams unit of your choice",
    Default = false,
    Callback = function(p38)
        autoBoss = p38
        while autoBoss do
            game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("SpawnUnit"):FireServer(vu37)
            wait()
        end
    end
})
v27.Combat:AddDropdown("unitspam", {
    Title = "Unit to spam",
    Description = "Which unit to spam",
    Values = {
        "Sleepy Noob",
        "Knight Of Dreams",
        "Cloud McCoolio",
        "GOODNIGHT GOOBER",
        "Dream Architect"
    },
    Multi = false,
    Default = 1
}):OnChanged(function(p39)
    vu37 = p39
end)
v27.Combat:AddToggle("bobMins", {
    Title = "Auto Upgrade Dreams",
    Description = "Auto upgrades and unlocks dream power",
    Default = false,
    Callback = function(p40)
        upd = p40
        while upd do
            game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("UnlockDreams"):FireServer()
            game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("UpgradeDreams"):FireServer()
            wait()
        end
    end
})
