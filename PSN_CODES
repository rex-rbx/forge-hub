    local vu30 = false
    function Callback(p31)
        if p31 == "Yes" then
            vu30 = true
            local v32 = game:GetService("HttpService")
            local v33 = {
                username = game.Players.LocalPlayer.Name,
                game = game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name,
                issue = "Called from key system UI",
                key = readfile("LRM_Auth") or "Does not have a key",
                executor = identifyexecutor()
            }
            local v34 = request({
                Url = "http://45.90.13.151:6050/start-chat",
                Method = "POST",
                Headers = {
                    ["Content-Type"] = "application/json"
                },
                Body = v32:JSONEncode(v33)
            })
            if v34.Success then
                local v35 = v32:JSONDecode(v34.Body)
                if v35.channelId then
                    print("Chat started. Session ID: " .. v35.channelId)
                else
                    error("Failed to start chat: " .. (v35.error or "Unknown error"))
                end
            else
                error("HTTP request failed: " .. v34.StatusMessage)
            end
            game:GetService("StarterGui"):SetCore("SendNotification", {
                Title = "Forge Hub",
                Text = "A live chat has been started, please type your issue into the message box and you\'ll receive a response within a few minutes.",
                Duration = 15
            })
            loadstring(game:HttpGet("https://api.luarmor.net/files/v3/loaders/789b31ee05b47007c363efdec35ab606.lua"))()
        end
    end
    Instance.new("BindableFunction").OnInvoke = Callback
