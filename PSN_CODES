            wait(3)
            vu10.PlaceholderText = "Enter Key"
        end
    end)
end
local _ = game.Players.LocalPlayer
if script_key then
    if game.CoreGui:FindFirstChild("FHBlr") then
        local v39 = game:GetService("TweenService")
        local v40 = TweenInfo.new(0.45)
        local v41 = {
            Size = UDim2.new(0.65, 0, 1, 0)
        }
        v39:Create(game.CoreGui.FHBlr.Frame.LoaderFrame.LD, v40, v41):Play()
    end
    spawn(function()
        wait(1)
        if game.CoreGui:FindFirstChild("FHBlr") then
            game.CoreGui.FHBlr:Destroy()
        end
    end)
    vu1 = true
else
    if isfile("LRM_Auth") then
        local v42 = loadstring(game:HttpGet("https://sdkapi-public.luarmor.net/library.lua"))()
        v42.script_id = "d52be677a99cd025dc4af965cd84d47a"
        local v43 = v42.check_key(readfile("LRM_Auth"))
        if v43.code ~= "KEY_VALID" then
            if v43.code ~= "KEY_HWID_LOCKED" then
                if v43.code ~= "KEY_INCORRECT" then
                    loadKs("Key check failed:" .. v43.message .. " Code: " .. v43.code)
                    delfile("LRM_Auth")
                else
                    loadKs("Your key has expired.")
                    delfile("LRM_Auth")
                end
            else
                loadKs("Your key is linked to a different HWID! You must join our discord at discord.gg/forgehubrbx to reset it.")
            end
        else
            script_key = readfile("LRM_Auth")
            vu1 = true
        end
    else
        loadKs()
    end
    repeat
        wait()
    until vu1 == true
end
