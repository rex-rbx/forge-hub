        v37.script_id = "d52be677a99cd025dc4af965cd84d47a"
        local v38 = v37.check_key(v36)
        if v38.code ~= "KEY_VALID" then
            if v38.code ~= "KEY_HWID_LOCKED" then
                if v38.code ~= "KEY_INCORRECT" then
                    if v38.code ~= "KEY_BANNED" then
                        vu10.Font = Enum.Font.GothamBold
                        vu10.TextColor3 = Color3.fromRGB(255, 0, 0)
                        vu10.Text = "Invalid Key!"
                        wait(3)
                        vu10.TextColor3 = Color3.fromRGB(255, 255, 255)
                        vu10.Text = ""
                        vu10.Font = Enum.Font.SourceSans
                    else
                        vu10.Font = Enum.Font.GothamBold
                        vu10.TextColor3 = Color3.fromRGB(255, 0, 0)
                        vu10.Text = "You are blacklisted from Forge Hub"
                        wait(1)
                        game.Players.LocalPlayer:Kick("\nYou\'re permanently blacklisted from Forge Hub! \n\nPlease join our discord server to appeal.")
                    end
                else
                    vu10.Font = Enum.Font.GothamBold
                    vu10.TextColor3 = Color3.fromRGB(255, 0, 0)
                    vu10.Text = "Key Invalid! It may be incorrectly typed, expired, or deleted."
                    wait(3)
                    vu10.TextColor3 = Color3.fromRGB(255, 255, 255)
                    vu10.Font = Enum.Font.SourceSans
                    vu10.Text = ""
                end
            else
                vu10.Font = Enum.Font.GothamBold
                vu10.TextColor3 = Color3.fromRGB(255, 0, 0)
                vu10.Text = "Key linked to different HWID! Please join our discord to reset it."
                wait(3)
                vu10.TextColor3 = Color3.fromRGB(255, 255, 255)
                vu10.Text = ""
                vu10.Font = Enum.Font.SourceSans
            end
        else
            vu10.TextColor3 = Color3.fromRGB(0, 255, 0)
            vu10.Text = "Key Is Valid! Loading Script.."
            vu10.Font = Enum.Font.GothamBold
            script_key = v36
            vu1 = true
            writefile("LRM_Auth", v36)
            wait(1)
            vu7:Destroy()
        end
    end)
    v13.Activated:Connect(function()
        if vu6 ~= "Normal" then
