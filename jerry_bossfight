v28.Combat:AddToggle("SlapAura", {
    Title = "Auto Slap",
    Description = "Auto slaps, you will need to turn this on to use other features",
    Default = false,
    Callback = function(p29)
        autoslap = p29
        while autoslap do
            if vu4.Character and vu4.Character:FindFirstChild("Default") then
                game:GetService("Players").LocalPlayer.Character.Default.RemoteFunction:InvokeServer(unpack({
                    "Attack"
                }))
            end
            wait()
        end
    end
})
v28.Combat:AddToggle("SlapAura", {
    Title = "Auto Attack Boss",
    Description = "Attacks the boss automatically",
    Default = false,
    Callback = function(p30)
        autoBoss = p30
        while autoBoss do
            print("In actual thing")
            if vu4 and vu4.Character and vu4.Character:FindFirstChild("Default") then
                print("nice 2nd")
                local v31, v32, v33 = pairs(vu4.Character.Default.Hitbox:GetChildren())
                while true do
                    local v34
                    v33, v34 = v31(v32, v33)
                    if v33 == nil then
                        break
                    end
                    if v34.Name == "TouchInterest" then
                        print("touch interest part")
                        local v35, v36, v37 = pairs(workspace:GetChildren())
                        while true do
                            local v38
                            v37, v38 = v35(v36, v37)
                            if v37 == nil then
                                break
                            end
                            if v38.Name == "Boss" and v38:FindFirstChild("HumanoidRootPart") then
                                local v39, v40, v41 = pairs(v38:GetChildren())
                                local v42 = nil
                                while true do
                                    local v43
                                    v41, v43 = v39(v40, v41)
                                    if v41 == nil then
                                        break
                                    end
                                    if v43:IsA("BasePart") and v43:FindFirstChild("LimbDeadParticle") then
                                        if v43.LimbDeadParticle.Enabled == false then
                                            v42 = v43
                                        end
                                    end
                                end
                                warn("check for dead is " .. tostring(v42))
                                if v42 == nil then
                                    warn("na it wasnt bruh")
                                    firetouchinterest(game.Players.LocalPlayer.Character.Default.Hitbox, v38.HumanoidRootPart, 0)
                                    firetouchinterest(game.Players.LocalPlayer.Character.Default.Hitbox, v38.HumanoidRootPart, 1)
                                else
                                    warn("CFD was ttue")
                                    firetouchinterest(game.Players.LocalPlayer.Character.Default.Hitbox, v42, 0)
                                    firetouchinterest(game.Players.LocalPlayer.Character.Default.Hitbox, v42, 1)
                                end
                            end
                        end
                    end
                end
            end
            wait()
        end
    end
})
v28.Combat:AddToggle("bobMins", {
    Title = "Auto Attack Snowman",
    Description = "Attacks jerry\'s snowmen automatically",
    Default = false,
    Callback = function(p44)
        autoMinion = p44
        while autoMinion do
            if vu4 and vu4.Character and vu4.Character:FindFirstChild("Default") then
                local v45, v46, v47 = pairs(vu4.Character.Default.Hitbox:GetChildren())
                while true do
                    local v48
                    v47, v48 = v45(v46, v47)
                    if v47 == nil then
                        break
                    end
                    if v48.Name == "TouchInterest" then
                        local v49, v50, v51 = pairs(workspace:GetChildren())
                        while true do
                            local v52
                            v51, v52 = v49(v50, v51)
                            if v51 == nil then
                                break
                            end
                            if v52.Name:find("SnowmanClone") and v52:FindFirstChild("HumanoidRootPart") then
                                firetouchinterest(game.Players.LocalPlayer.Character.Default.Hitbox, v52.HumanoidRootPart, 0)
                                firetouchinterest(game.Players.LocalPlayer.Character.Default.Hitbox, v52.HumanoidRootPart, 1)
                            end
                        end
                    end
                end
            end
            wait()
        end
    end
})
