v28.Player:AddToggle("ATC", {
    Title = "Anti Ragdoll [Semi]",
    Description = "Makes it so you can\'t ragdoll [Is a little bit broken]",
    Default = false,
    Callback = function(p55)
        AntiRagdoll = p55
        while AntiRagdoll do
            if game.Players.LocalPlayer.Character and (game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart") and workspace.Camera:FindFirstChild(vu4.Name .. "Ragdoll")) then
                repeat
                    task.wait()
                    game.Players.LocalPlayer.Character.Torso.Anchored = true
                    print("in tat")
                until workspace.Camera:FindFirstChild(vu4.Name .. "Ragdoll") == nil
                game.Players.LocalPlayer.Character.Torso.Anchored = false
                print("false now")
            end
            task.wait()
        end
    end
})
v28.Player:AddButton({
    Title = "Anti Screen Shake",
    Description = "Removes screen shake permanently",
    Callback = function()
        local v56 = game.Players.LocalPlayer
        workspace.CurrentCamera:remove()
        repeat
            wait()
        until v56.Character ~= nil
        workspace.CurrentCamera.CameraSubject = v56.Character:FindFirstChildWhichIsA("Humanoid")
        workspace.CurrentCamera.CameraType = "Custom"
        v56.CameraMinZoomDistance = 0.5
        v56.CameraMaxZoomDistance = 400
        v56.CameraMode = "Classic"
        v56.Character.Head.Anchored = false
    end
})
v28.Player:AddButton({
    Title = "Remove Attacks",
    Description = "Completely removes Jerry\'s attacks such as Icicles and walls",
    Callback = function()
        if workspace:FindFirstChild("Attacks") then
            workspace.Attacks:Destroy()
        end
        spawn(function()
            while true do
                if game:GetService("SoundService"):FindFirstChild("Effects") and game:GetService("SoundService"):FindFirstChild("Effects"):FindFirstChild("Warning") then
                    game:GetService("SoundService"):FindFirstChild("Effects"):FindFirstChild("Warning"):Destroy()
                end
                if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("Indicator") then
                    game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("Indicator"):Destroy()
                end
                wait()
            end
        end)
    end
})
